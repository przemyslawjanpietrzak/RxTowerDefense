{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "tabs", 1],
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "variable-name": false,
        "max-line-length": [true, 150],
        "member-ordering": false
        
    },
    "rulesDirectory": []
}