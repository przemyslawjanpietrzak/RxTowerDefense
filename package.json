{
  "name": "RxTowerDefence",
  "version": "0.1.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "lint": "tslint --config tslint.json --project tsconfig.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "bundle": "webpack --config ./webpack.config.js",
    "bundle:production": "webpack --config ./webpack.config.js",
    "getTheme": "mkdir -p dist/fonts && wget https://raw.githubusercontent.com/kristopolous/BOOTSTRA.386/master/v2.3.1/bootstrap/css/bootstrap.css -O dist/bootstrap.css && wget https://github.com/kristopolous/BOOTSTRA.386/raw/master/v2.3.1/bootstrap/css/fonts/Fixedsys500c.woff -O dist/fonts/Fixedsys500c.woff",
    "copy": "cp index.html dist/",
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run bundle:production && yarn run getTheme && yarn run copy",
    "deploy:develop": "aws s3 cp dist/ s3://rxtd-develop --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --recursive",
    "deploy:master": "aws s3 cp dist/ s3://rxtd-master --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --recursive",
    "deploy:pull-request": "aws s3 cp dist/ s3://rxtd-pr --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --recursive",
    "deploy": "if [ \"$TRAVIS_BRANCH\" -eq \"develop\" ]; then yarn run delpoy:develop; elif [ $TRAVIS_BRANCH == \"master\" ]; then yarn run delpoy:master; else yarn run deploy:pull-request; fi"
  },
  "author": "Przemyslaw Jan Pietrzak",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "mocha": "5.0.0",
    "ts-loader": "3.4.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "5.1.0",
    "typescript": "2.7.1",
    "typings": "2.1.1",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "easeljs": "1.0.2",
    "rxjs": "5.5.6",
    "victor": "1.1.0"
  },
  "resolutions": {
    "typescript": "2.7.1",
    "commander": "2.15.0",
    "debug": "3.1.0",
    "diff": "3.3.1",
    "supports-color": "5.3.0",
    "inherits": "2.0.3",
    "minimist": "1.2.0",
    "has-flag": "3.0.0",
    "chalk": "2.3.2",
    "semver": "5.5.0",
    "ansi-styles": "3.2.1",
    "string_decoder": "1.1.0",
    "define-property": "2.0.2",
    "extend-shallow": "3.0.2",
    "kind-of": "6.0.2",
    "isobject": "3.0.1",
    "is-descriptor": "1.0.2",
    "is-accessor-descriptor": "1.0.0",
    "is-data-descriptor": "1.0.0",
    "is-extendable": "1.0.1",
    "is-number": "4.0.0",
    "is-windows": "1.0.2",
    "source-map": "0.6.1",
    "has-value": "1.0.0",
    "set-value": "2.0.0",
    "has-values": "1.0.0",
    "strip-ansi": "3.0.1",
    "ansi-regex": "3.0.0",
    "wordwrap": "1.0.0",
    "string-width": "2.1.1",
    "is-fullwidth-code-point": "2.0.0",
    "pify": "3.0.0",
    "form-data": "2.3.2",
    "punycode": "1.4.1",
    "nopt": "4.0.1",
    "camelcase": "4.1.0",
    "acorn": "5.5.3",
    "ajv": "5.5.2",
    "yargs": "8.0.2",
    "hash-base": "3.0.4",
    "cliui": "3.2.0",
